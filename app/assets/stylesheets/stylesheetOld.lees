/* --------------------------------------------- MAIN PAGE --------------------------------------------- */
.mainpage{

  background: url("/assets/images/stripeBackground.png") repeat;
  position: absolute;
  width:100%;
  height: 100%;

  .row.subtitle{
    text-align: center;
    margin-top: 30px;
    margin-left: 0;

    span{
      color: gray;
      font-family: love;
      font-size: 20px;
    }
  }

  .row.boxes{
    margin-top: 10%;

  }

  .btn.success, .alert-message.success {
    .ButtonOrange;
  }

  h1{
    color: gray;
    //text-shadow: 3px 0px black;
    letter-spacing: 6px;
    text-transform: uppercase;
    font-family: RoyalDelightShad;
    font-size: 50px;
    text-align: center;
    padding-top: 0;
  }

  .form-signin {
    max-width: 300px;
    padding: 19px 29px 29px;

    margin: 20px auto auto 20px;
    background: rgba(245, 122, 122, 0.7);
    .Border-radius(500px);
    -webkit-box-shadow: 0 0 10px 10px rgba(0,0,0,0.3);
    -moz-box-shadow: 0 0 10px 10px rgba(0,0,0,0.3);
    box-shadow: 0 0 10px 10px rgba(0,0,0,0.3);
    box-shadow: 0 0 10px 10px rgba(0,0,0,0.3);
    border: solid rgba(255, 204, 112, 0.5);
    text-align: center;
  }
  .form-signin .form-signin-heading,
  .form-signin .checkbox {
    margin-bottom: 10px;
    color: #f5f5f5;
    font-family: love;
  }
  .form-signin input[type="text"],
  .form-signin input[type="password"] {
    font-size: 16px;
    height: auto;
    margin-bottom: 15px;
    padding: 7px 9px;
  }

}

.btn.btn-login {
  color: #ffffff;
  text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
  background-color: rgba(245, 122, 122, 0.7);
  *background-color: #f46868;
  background-image: -moz-linear-gradient(top, #f57a7a, #f46868);
  background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#f57a7a), to(#f46868));
  background-image: -webkit-linear-gradient(top, #f57a7a, #f46868);
  background-image: -o-linear-gradient(top, #f57a7a, #f46868);
  background-image: linear-gradient(to bottom, #f57a7a, #f46868);
  background-repeat: repeat-x;
  border-color: #f57a7a #f57a7a #f46868;
  border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ff62c462', endColorstr='#ff51a351', GradientType=0);
  filter: progid:DXImageTransform.Microsoft.gradient(enabled=false);
}

.btn.btn-login:hover,
.btn.btn-login:focus,
.btn.btn-login:active,
.btn.btn-login.active,
.btn.btn-login.disabled,
.btn.btn-login[disabled] {
  color: #ffffff;
  background-color: #f57a7a;
  *background-color: #f46868;
}

.help-inline {
  color: lightcoral;
  font-size: 12px;
  font-style: oblique;
}



