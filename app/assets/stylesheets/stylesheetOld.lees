@orange: #FF9933;
@white: white;

.Border-radius(@radius:5px) {
  -webkit-border-radius:@radius;
  -moz-border-radius: @radius;
  border-radius: @radius;
}

.ButtonOrange{
    .BackgroundOrange;
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#FFAD5c', endColorstr='#FF9933', GradientType=0);
    text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
    border-color: #FFAD5c #FFAD5c #FF9933;
    border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
}

.BackgroundOrange{
  background-color: @orange;
  background-repeat: repeat-x;
  background-image: -khtml-gradient(linear, left top, left bottom, from(#FFAD5c), to(#FF9933));
  background-image: -moz-linear-gradient(top, #FFAD5c, #FF9933);
  background-image: -ms-linear-gradient(top, #FFAD5c, #FF9933);
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #FF9933), color-stop(100%, #FF9933));
  background-image: -webkit-linear-gradient(top, #FFAD5c, #FF9933);
  background-image: -o-linear-gradient(top, #FFAD5c, #FF9933);
  background-image: linear-gradient(top, #FFAD5c, #FF9933);
}

.underline{
  text-decoration: underline;
}

.inline{
  display: inline;
}

