  text-align: center;
  padding: 30px 0;
  border-top: 1px solid #e5e5e5;
  background-color: #f5f5f5;
  height:20px;
  bottom: 0;
  width: 100%;

}

.header{
  text-align: center;
  padding: 15px;
  background-color:gray;
  box-shadow: 0 0 25px 4px rgba(0,0,0,0.3);
}

.wraparound {
  position: relative;
  margin: 0 -11px;
  box-shadow: 0 4px 4px rgba(0,0,0,0.3);
  &:before , &:after {
    content: " ";
    border-top: 10px solid gray;
    position: absolute;
    bottom: -11px;
  }
  &:before {
    border-left: 10px solid transparent;
    left: 0;
  }
  &:after {
    border-right: 10px solid transparent;
    right: 0;
  }
}

.container .bodyPage{
  background-color: white;
  padding-left: 20px;
  padding-right: 20px;
  text-align: justify;
  box-shadow: 0 0 25px 4px rgba(0,0,0,0.3);
  padding-top: 10px;


}
