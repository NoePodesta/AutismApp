#profileImage{

  .modal-content {
    width: 600px;
    height: 700px;
  }
  .modal-dialog {
    width: 600px;
    height: 700px;
  }
  .modal-header {
    padding: 0;
    border: none;
  }
  .modal-body {
    width: 515px;
    height: 650px;
    max-height: none;
    overflow-y: hidden;
    overflow-x: hidden;
  }
  .close {
    background-image: url("/assets/images/close.png");
    position: absolute;
    border: none;
    width: 30px;
    height: 30px;
    background-repeat: no-repeat;
    left: -20px;
    top: -20px;
    opacity: 1;
  }
}


